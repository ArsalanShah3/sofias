    <button id="prev" class="nav-btn nav-left disabled" aria-label="previous">
      <svg viewBox="0 0 24 24"><path d="M15 18L9 12l6-6" stroke-linecap="round" stroke-linejoin="round"/></svg>
    </button>
    <button id="next" class="nav-btn nav-right" aria-label="next">
      <svg viewBox="0 0 24 24"><path d="M9 6l6 6-6 6" stroke-linecap="round" stroke-linejoin="round"/></svg>
    </button>
