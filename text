  <script>
    (function(){
      const track = document.getElementById('track');
      const prevBtn = document.getElementById('prev');
      const nextBtn = document.getElementById('next');
      const cards = Array.from(track.children);
      let index = 0;

      function slide(){
        const gap = 28; // fixed gap as per CSS
        const cardWidth = 320; // fixed card width as per CSS
        
        const translateX = -index * (cardWidth + gap);
        track.style.transform = `translateX(${translateX}px)`;

        // Update buttons
        prevBtn.classList.toggle('disabled', index <= 0);
        nextBtn.classList.toggle('disabled', index >= cards.length - 3);
      }

      nextBtn.addEventListener('click', () => {
        if(index < cards.length - 3){
          index++;
          slide();
        }
      });

      prevBtn.addEventListener('click', () => {
        if(index > 0){
          index--;
          slide();
        }
      });

      // Handle responsive adjustments
      function handleResize(){
        const viewport = track.parentElement;
        if(window.innerWidth <= 900){
          viewport.style.width = '100%';
        } else {
          viewport.style.width = 'calc(3 * 320px + 2 * 28px)';
          if(window.innerWidth <= 1100){
            viewport.style.width = 'calc(3 * 280px + 2 * 28px)';
          }
        }
        slide();
      }

      window.addEventListener('load', handleResize);
      window.addEventListener('resize', handleResize);

      // init immediately
      slide();

    })();
  </script>

